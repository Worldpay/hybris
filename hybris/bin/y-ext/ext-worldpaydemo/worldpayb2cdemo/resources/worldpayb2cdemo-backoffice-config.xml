<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config xmlns="http://www.hybris.com/cockpit/config"
        xmlns:explorer-tree="http://www.hybris.com/cockpitng/config/explorertree"
        xmlns:list-view="http://www.hybris.com/cockpitng/component/listView"
        xmlns:y="http://www.hybris.com/cockpit/config/hybris"
        xmlns:editorArea="http://www.hybris.com/cockpitng/component/editorArea">

    <context component="explorer-tree" merge-by="module">
       <explorer-tree:explorer-tree>
           <explorer-tree:navigation-node id="Worldpay" merge-mode="append">
               <explorer-tree:type-node code="WorldpayBINRange" id="worldpay.binranges"/>
           </explorer-tree:navigation-node>
       </explorer-tree:explorer-tree>
    </context>

    <context merge-by="type" type="WorldpayBinRange" component="advanced-search">
        <advanced-search:advanced-search xmlns:advanced-search="http://www.hybris.com/cockpitng/config/advancedsearch">
            <advanced-search:field-list>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="CardBinRangeStartOriginal" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="CardBinRangeEndOriginal" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="CardBinRangeStart" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="CardBinRangeEnd" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="CardName" selected="false" operator="contains"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="CardType" selected="true" operator="contains"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="CountryCode" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="CardIssuer" selected="false" operator="contains"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="CardLength" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="IssueNoLength" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="IssueNoRequired" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="Notes" selected="false" operator="contains"/>
            </advanced-search:field-list>
        </advanced-search:advanced-search>
    </context>

    <context merge-by="type" parent="GenericItem" type="WorldpayBinRange" component="listview">
        <list-view:list-view>
            <list-view:column qualifier="CardBinRangeStartOriginal"/>
            <list-view:column qualifier="CardBinRangeEndOriginal"/>
            <list-view:column qualifier="CardBinRangeStart"/>
            <list-view:column qualifier="CardBinRangeEnd"/>
            <list-view:column qualifier="CardName"/>
            <list-view:column qualifier="CardType"/>
            <list-view:column qualifier="CountryCode"/>
            <list-view:column qualifier="CardIssuer"/>
            <list-view:column qualifier="CardLength"/>
            <list-view:column qualifier="IssueNoLength"/>
            <list-view:column qualifier="IssueNoRequired"/>
            <list-view:column qualifier="Notes"/>
        </list-view:list-view>
    </context>

    <context type="WorldpayBinRange" component="editor-area" merge-by="type" parent="GenericItem">
        <editorArea:editorArea name="" xmlns:editorArea="http://www.hybris.com/cockpitng/component/editorArea">
            <editorArea:essentials xmlns:editorArea="http://www.hybris.com/cockpitng/component/editorArea">
                <editorArea:essentialSection name="hmc.essential">
                    <editorArea:attribute qualifier="CardBinRangeStartOriginal"/>
                    <editorArea:attribute qualifier="CardBinRangeEndOriginal"/>
                    <editorArea:attribute qualifier="CardBinRangeStart"/>
                    <editorArea:attribute qualifier="CardBinRangeEnd"/>
                    <editorArea:attribute qualifier="CardName"/>
                    <editorArea:attribute qualifier="CardType"/>
                    <editorArea:attribute qualifier="CountryCode"/>
                    <editorArea:attribute qualifier="CardIssuer"/>
                    <editorArea:attribute qualifier="CardLength"/>
                    <editorArea:attribute qualifier="IssueNoLength"/>
                    <editorArea:attribute qualifier="IssueNoRequired"/>
                    <editorArea:attribute qualifier="Notes"/>
                </editorArea:essentialSection>
            </editorArea:essentials>
        </editorArea:editorArea>
    </context>

    <context type="WorldpayBinRange" component="base" merge-by="type">
        <y:base>
            <y:labels>
                <y:label>'CardType: ' + cardType + ', CardName: ' + cardName + ', CountryCode: ' + countryCode + ', CardIssuer: ' + cardIssuer</y:label>
            </y:labels>
        </y:base>
    </context>

    <context merge-by="type" type="IPInformation" component="advanced-search">
        <advanced-search:advanced-search xmlns:advanced-search="http://www.hybris.com/cockpitng/config/advancedsearch">
            <advanced-search:field-list>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="isp" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="org" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="lat" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="lon" selected="true"/>
            </advanced-search:field-list>
        </advanced-search:advanced-search>
    </context>

    <context merge-by="type" parent="GenericItem" type="IPInformation" component="listview">
        <list-view:list-view>
            <list-view:column qualifier="isp"/>
            <list-view:column qualifier="org"/>
            <list-view:column qualifier="lat"/>
            <list-view:column qualifier="lon"/>
        </list-view:list-view>
    </context>

    <context type="IPInformation" component="editor-area" merge-by="type" parent="GenericItem">
        <editorArea:editorArea name="" xmlns:editorArea="http://www.hybris.com/cockpitng/component/editorArea">
            <editorArea:essentials xmlns:editorArea="http://www.hybris.com/cockpitng/component/editorArea">
                <editorArea:essentialSection name="hmc.essential">
                    <editorArea:attribute qualifier="isp"/>
                    <editorArea:attribute qualifier="org"/>
                    <editorArea:attribute qualifier="lat"/>
                    <editorArea:attribute qualifier="lon"/>
                </editorArea:essentialSection>
            </editorArea:essentials>
        </editorArea:editorArea>
    </context>

    <context type="IPInformation" component="base" merge-by="type">
        <y:base>
            <y:labels>
                <y:label>'isp: ' + isp + ', org: ' + org + ', latitude: ' + lat + ', longitude: ' + lon</y:label>
            </y:labels>
        </y:base>
    </context>

    <context merge-by="type" type="WeatherInformation" component="advanced-search">
        <advanced-search:advanced-search xmlns:advanced-search="http://www.hybris.com/cockpitng/config/advancedsearch">
            <advanced-search:field-list>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="date" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="cityName" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="country" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="longitude" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="latitude" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="weather" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="temperature" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="pressure" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="humidity" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="minTemp" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="maxTemp" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="seaLevel" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="groundLevel" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="windSpeed" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="windDegree" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="clouds" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="rain" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="snow" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="sunrise" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="sunset" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="cityId" selected="false"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="code" selected="false"/>
            </advanced-search:field-list>
        </advanced-search:advanced-search>
    </context>

    <context type="WeatherInformation" component="base" merge-by="type">
        <y:base>
            <y:labels>
                <y:label>'Date: ' + dt + ', City: ' + name + ', Country: ' + country</y:label>
            </y:labels>
        </y:base>
    </context>

    <context merge-by="type" type="Weather" component="advanced-search">
        <advanced-search:advanced-search xmlns:advanced-search="http://www.hybris.com/cockpitng/config/advancedsearch">
            <advanced-search:field-list>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="weatherId" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="main" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="description" selected="true"/>
                <advanced-search:field xmlns="http://www.hybris.com/cockpitng/config/advancedsearch" name="iconId" selected="false"/>
            </advanced-search:field-list>
        </advanced-search:advanced-search>
    </context>

    <context type="Weather" component="base" merge-by="type">
        <y:base>
            <y:labels>
                <y:label>'Id: ' + id + ', Description:' + description</y:label>
            </y:labels>
        </y:base>
    </context>

    <context type="Weather" component="editor-area" merge-by="type" parent="GenericItem">
        <editorArea:editorArea name="" xmlns:editorArea="http://www.hybris.com/cockpitng/component/editorArea">
            <editorArea:essentials xmlns:editorArea="http://www.hybris.com/cockpitng/component/editorArea">
                <editorArea:essentialSection name="hmc.essential">
                    <editorArea:attribute qualifier="description"/>
                    <editorArea:attribute qualifier="id"/>
                    <editorArea:attribute qualifier="main"/>
                    <editorArea:custom>
                        <editorArea:default>
                            <![CDATA[<img src="http://openweathermap.org/img/w/{icon}.png" onerror="this.style.display='none'"/>]]>
                        </editorArea:default>
                    </editorArea:custom>
                </editorArea:essentialSection>
            </editorArea:essentials>
        </editorArea:editorArea>
    </context>

</config>
