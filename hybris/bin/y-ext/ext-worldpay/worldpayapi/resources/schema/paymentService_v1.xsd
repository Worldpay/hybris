    <xs:element name="autoRenew" type="xs:string"/>
    <!-- marketplace -->
    <xs:element name="marketplaceSeller">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="marketplaceAccountIdentifier"/>
                <xs:element ref="subMerchantId"/>
                <xs:element ref="category"/>
                <xs:element ref="productName"/>
                <xs:element ref="registrationDate"/>
                <xs:element ref="lastModified"/>
                <xs:element ref="rating"/>
                <xs:element ref="tradeCount"/>
                <xs:element ref="tradeVolume"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="marketplaceBuyer">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="marketplaceAccountIdentifier"/>
                <xs:element ref="registrationDate"/>
                <xs:element ref="lastModified"/>
                <xs:element ref="tradeCount"/>
                <xs:element ref="tradeVolume"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="marketplaceCustomer">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="marketplaceAccountIdentifier"/>
                <xs:element minOccurs="0" ref="registrationDate"/>
                <xs:element minOccurs="0" ref="lastModified"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="marketplaceCustomerPayment">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="marketplaceAccountIdentifier"/>
                <xs:element minOccurs="0" ref="paymentOption"/>
                <xs:element minOccurs="0" ref="paidPurchaseCount"/>
                <xs:element minOccurs="0" ref="paidPurchaseTotal"/>
                <xs:element minOccurs="0" ref="lastPaidPurchase"/>
                <xs:element minOccurs="0" ref="firstPaidPurchase"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="marketplaceCustomerPaymentSummary">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="marketplaceAccountIdentifier"/>
                <xs:element minOccurs="0" ref="hasPayment"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- alternate delivery address -->
    <xs:element name="alternativeShippingAddress">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="shippingSummary"/>
                <xs:element minOccurs="0" ref="address"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="shippingSummary">
        <xs:complexType>
            <xs:attribute name="shippingMethod" use="required"/>
            <xs:attribute name="shippingType" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="marketplaceAccountIdentifier" type="xs:string"/>
    <xs:element name="subMerchantId" type="xs:string"/>
    <xs:element name="category" type="xs:string"/>
    <xs:element name="productName" type="xs:string"/>
    <xs:element name="registrationDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="lastModified">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="rating" type="xs:string"/>
    <xs:element name="tradeCount" type="xs:string"/>
    <xs:element name="tradeVolume" type="xs:string"/>
    <xs:element name="paidPurchaseCount" type="xs:string"/>
    <xs:element name="paidPurchaseTotal" type="xs:string"/>
    <xs:element name="lastPaidPurchase">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="firstPaidPurchase">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="hasPayment" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ALL PURCHASE ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="purchase">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="invoiceReferenceNumber"/>
                <xs:element minOccurs="0" ref="customerReference"/>
                <xs:element minOccurs="0" ref="cardAcceptorTaxId"/>
                <xs:element minOccurs="0" ref="salesTax"/>
                <xs:element minOccurs="0" ref="discountName"/>
                <xs:element minOccurs="0" ref="discountCode"/>
                <xs:element minOccurs="0" ref="discountAmount"/>
                <xs:element minOccurs="0" ref="shippingAmount"/>
                <xs:element minOccurs="0" ref="dutyAmount"/>
                <xs:element minOccurs="0" ref="shipFromPostalCode"/>
                <xs:element minOccurs="0" ref="destinationPostalCode"/>
                <xs:element minOccurs="0" ref="destinationCountryCode"/>
                <xs:element minOccurs="0" ref="orderDate"/>
                <xs:element minOccurs="0" ref="shippingDate"/>
                <xs:element minOccurs="0" ref="taxExempt"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="item"/>
                <xs:element minOccurs="0" ref="shippingCourier"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="item">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="description"/>
                <xs:element minOccurs="0" ref="productCode"/>
                <xs:element minOccurs="0" ref="commodityCode"/>
                <xs:element minOccurs="0" ref="quantity"/>
                <xs:element minOccurs="0" ref="unitCost"/>
                <xs:element minOccurs="0" ref="unitOfMeasure"/>
                <xs:element minOccurs="0" ref="itemTotal"/>
                <xs:element minOccurs="0" ref="itemTotalWithTax"/>
                <xs:element minOccurs="0" ref="itemDiscountAmount"/>
                <xs:element minOccurs="0" ref="taxAmount"/>
                <xs:element minOccurs="0" ref="categories"/>
                <xs:element minOccurs="0" ref="pageURL"/>
                <xs:element minOccurs="0" ref="imageURL"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="shippingCourier">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="name"/>
                <xs:element minOccurs="0" ref="trackingNumber"/>
                <xs:element minOccurs="0" ref="priority"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="categories">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="category"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- GRANDCHILD ELEMENTS OF ALL PURCHASE ELEMENTS -->
    <!-- ================================= -->
    <!--
        if Vatnumber doesn't start with two character language code, it may be
        prefixed with the destination CountryCode in the case of the Shopper and
        with the departure countryCode in case of the merchant VAT number
    -->
    <xs:element name="customerReference" type="xs:string"/>
    <xs:element name="invoiceReferenceNumber" type="xs:string"/>
    <xs:element name="cardAcceptorTaxId" type="xs:string"/>
    <xs:element name="salesTax">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="discountName" type="xs:string"/>
    <xs:element name="discountAmount">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="discountCode" type="xs:string"/>
    <xs:element name="shippingAmount">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="dutyAmount">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="shipFromPostalCode" type="xs:string"/>
    <xs:element name="destinationPostalCode" type="xs:string"/>
    <xs:element name="destinationCountryCode" type="xs:string"/>
    <xs:element name="orderDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="shippingDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="taxExempt" type="xs:string"/>
    <xs:element name="productCode" type="xs:string"/>
    <xs:element name="commodityCode" type="xs:string"/>
    <xs:element name="unitCost">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="unitOfMeasure" type="xs:string"/>
    <xs:element name="taxAmount">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="itemTotal">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="itemTotalWithTax">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="itemDiscountAmount">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="pageURL" type="xs:string"/>
    <xs:element name="trackingNumber" type="xs:string"/>
    <xs:element name="priority" type="xs:string"/>
    <!--
        GENERIC ELEMENT FOR SUBMITTING RESULT URLS FOR AN ORDER,
        CURRENLTLY ONLY USED FOR CREDIT CARD 3rd PARTY MPI INTEGRATION IN TURKEY.
    -->
    <xs:element name="resultURL">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="successURL"/>
                <xs:element minOccurs="0" ref="failureURL"/>
                <xs:element minOccurs="0" ref="errorURL"/>
                <xs:element minOccurs="0" ref="pendingURL"/>
                <xs:element minOccurs="0" ref="cancelURL"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="riskData">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="authenticationRiskData"/>
                <xs:element minOccurs="0" ref="shopperAccountRiskData"/>
                <xs:element minOccurs="0" ref="transactionRiskData"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="authenticationRiskData">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="authenticationTimestamp"/>
                <xs:element minOccurs="0" ref="alternateAuthenticationData"/>
            </xs:sequence>
            <xs:attribute name="authenticationMethod">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="guestCheckout"/>
                        <xs:enumeration value="localAccount"/>
                        <xs:enumeration value="federatedAccount"/>
                        <xs:enumeration value="issuerCredentials"/>
                        <xs:enumeration value="thirdPartyAuthentication"/>
                        <xs:enumeration value="fidoAuthenticator"/>
                        <xs:enumeration value="fidoAuthenticatorSignedData"/>
                        <xs:enumeration value="assuranceData"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="authenticationTimestamp">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="alternateAuthenticationData">
        <xs:complexType mixed="true">
            <xs:attribute name="method">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="staticPasscode"/>
                        <xs:enumeration value="smsOTP"/>
                        <xs:enumeration value="physicalReaderOTP"/>
                        <xs:enumeration value="appOTP"/>
                        <xs:enumeration value="otherOTP"/>
                        <xs:enumeration value="KBA"/>
                        <xs:enumeration value="biometricsOOB"/>
                        <xs:enumeration value="loginOOB"/>
                        <xs:enumeration value="otherOOB"/>
                        <xs:enumeration value="other"/>
                        <xs:enumeration value="pushConfirmation"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="shopperAccountRiskData">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="shopperAccountCreationDate"/>
                <xs:element minOccurs="0" ref="shopperAccountModificationDate"/>
                <xs:element minOccurs="0" ref="shopperAccountPasswordChangeDate"/>
                <xs:element minOccurs="0" ref="shopperAccountShippingAddressFirstUseDate"/>
                <xs:element minOccurs="0" ref="shopperAccountPaymentAccountFirstUseDate"/>
            </xs:sequence>
            <xs:attribute name="transactionsAttemptedLastDay" type="xs:NMTOKEN"/>
            <xs:attribute name="transactionsAttemptedLastYear" type="xs:NMTOKEN"/>
            <xs:attribute name="purchasesCompletedLastSixMonths" type="xs:NMTOKEN"/>
            <xs:attribute name="addCardAttemptsLastDay" type="xs:NMTOKEN"/>
            <xs:attribute name="previousSuspiciousActivity">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="shippingNameMatchesAccountName">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="shopperAccountAgeIndicator">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="noAccount"/>
                        <xs:enumeration value="createdDuringTransaction"/>
                        <xs:enumeration value="lessThanThirtyDays"/>
                        <xs:enumeration value="thirtyToSixtyDays"/>
                        <xs:enumeration value="moreThanSixtyDays"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="shopperAccountChangeIndicator">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="changedDuringTransaction"/>
                        <xs:enumeration value="lessThanThirtyDays"/>
                        <xs:enumeration value="thirtyToSixtyDays"/>
                        <xs:enumeration value="moreThanSixtyDays"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="shopperAccountPasswordChangeIndicator">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="noChange"/>
                        <xs:enumeration value="changedDuringTransaction"/>
                        <xs:enumeration value="lessThanThirtyDays"/>
                        <xs:enumeration value="thirtyToSixtyDays"/>
                        <xs:enumeration value="moreThanSixtyDays"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="shopperAccountShippingAddressUsageIndicator">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="thisTransaction"/>
                        <xs:enumeration value="lessThanThirtyDays"/>
                        <xs:enumeration value="thirtyToSixtyDays"/>
                        <xs:enumeration value="moreThanSixtyDays"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="shopperAccountPaymentAccountIndicator">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="noAccount"/>
                        <xs:enumeration value="duringTransaction"/>
                        <xs:enumeration value="lessThanThirtyDays"/>
                        <xs:enumeration value="thirtyToSixtyDays"/>
                        <xs:enumeration value="moreThanSixtyDays"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="shopperAccountCreationDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="shopperAccountModificationDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="shopperAccountPasswordChangeDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="shopperAccountShippingAddressFirstUseDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="shopperAccountPaymentAccountFirstUseDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="transactionRiskData">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="transactionRiskDataGiftCardAmount"/>
                <xs:element minOccurs="0" ref="transactionRiskDataPreOrderDate"/>
            </xs:sequence>
            <xs:attribute name="shippingMethod">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="shipToBillingAddress"/>
                        <xs:enumeration value="shipToVerifiedAddress"/>
                        <xs:enumeration value="shipToOtherAddress"/>
                        <xs:enumeration value="shipToStore"/>
                        <xs:enumeration value="digital"/>
                        <xs:enumeration value="unshippedTravelOrEventTickets"/>
                        <xs:enumeration value="other"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="deliveryTimeframe">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="electronicDelivery"/>
                        <xs:enumeration value="sameDayShipping"/>
                        <xs:enumeration value="overnightShipping"/>
                        <xs:enumeration value="otherShipping"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="deliveryEmailAddress"/>
            <xs:attribute name="reorderingPreviousPurchases">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="preOrderPurchase">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
