    <xs:element name="issueNumber" type="xs:string"/>
    <!-- CHILD ELEMENT OF paymentDetails ELEMENT -->
    <xs:element name="POSRequest">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="ISO8583Request"/>
                <xs:element ref="WeChatPay"/>
            </xs:choice>
            <xs:attribute name="isRetry" default="false">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="isOfflineAuth" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="requestBalance" default="false">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="allowPartialAuth" default="false">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="isDeferredAuth" default="false">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="ISO8583Request">
        <xs:complexType>
            <xs:sequence>
                <xs:choice>
                    <xs:sequence>
                        <xs:element ref="cardNumber"/>
                        <xs:element ref="expiryDate"/>
                    </xs:sequence>
                    <xs:element ref="encryptedData"/>
                </xs:choice>
                <xs:element ref="transmissionDate"/>
                <xs:element ref="systemsTraceAuditNumber"/>
                <xs:element ref="localDateTimeAtPOS"/>
                <xs:element minOccurs="0" ref="originalLocalDateTimeAtPOS"/>
                <xs:element ref="POSEntryMode"/>
                <xs:element ref="POSConditionCode"/>
                <xs:element minOccurs="0" ref="track"/>
                <xs:element ref="terminalData"/>
                <xs:element minOccurs="0" ref="applicationPANSequenceNumber"/>
                <xs:element minOccurs="0" ref="CVMR"/>
                <xs:element minOccurs="0" ref="reasonForOnline"/>
                <xs:element minOccurs="0" ref="ISOResponseIndicator"/>
                <xs:element minOccurs="0" ref="AuthorisationId"/>
                <xs:element minOccurs="0" ref="authorisationResponseCode"/>
                <xs:element ref="acquirerMerchantId"/>
                <xs:element minOccurs="0" ref="issuerCardData"/>
                <xs:element minOccurs="0" ref="cardholderIdentificationMethod"/>
                <xs:element minOccurs="0" ref="EMVRequest"/>
                <xs:element minOccurs="0" ref="onlinePIN"/>
                <xs:element minOccurs="0" ref="singleTapPIN"/>
            </xs:sequence>
            <xs:attribute name="chipCard" default="false">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
