    <xs:element name="company" type="xs:string"/>
    <xs:element name="eventType" type="xs:string"/>
    <xs:element name="ticketed" type="xs:string"/>
    <!-- voucher -->
    <xs:element name="voucher">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="name"/>
                <xs:element minOccurs="0" ref="company"/>
                <xs:element minOccurs="0" ref="start"/>
                <xs:element minOccurs="0" ref="end"/>
                <xs:element minOccurs="0" ref="affiliateName"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- subscription -->
    <xs:element name="subscription">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="name"/>
                <xs:element minOccurs="0" ref="start"/>
                <xs:element minOccurs="0" ref="end"/>
                <xs:element minOccurs="0" ref="autoRenew"/>
                <xs:element minOccurs="0" ref="affiliateName"/>
