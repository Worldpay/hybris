                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!--
        The cancel immediate attribute can only used for EMV transactions.
        If the merchant is not participating in EMV the value is not checked.
        When the value is set to 'yes', a cancel will be processed directly,
        and not saved as batch.
        Of course the Shoppercell of the authorisation and the cancel should
        be the same!
    -->
    <xs:element name="cancelOrRefund">
        <xs:complexType/>
    </xs:element>
    <xs:element name="POSCaptureRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:sequence minOccurs="0">
                    <xs:element ref="cryptogram"/>
                    <xs:element ref="ICCCryptogramInformation"/>
                </xs:sequence>
                <xs:element minOccurs="0" ref="transactionStatusInfo"/>
            </xs:sequence>
            <xs:attribute name="isRetry" default="false">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="capture">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="localDateTimeAtPOS"/>
                <xs:element minOccurs="0" ref="date"/>
                <xs:element ref="amount"/>
                <xs:element minOccurs="0" ref="branchSpecificExtension"/>
                <xs:element minOccurs="0" ref="POSCaptureRequest"/>
                <xs:element minOccurs="0" ref="shipping"/>
                <xs:element minOccurs="0" ref="orderLines"/>
            </xs:sequence>
            <xs:attribute name="reference"/>
            <xs:attribute name="instant">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="sequenceNumber"/>
            <xs:attribute name="totalCount"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="defend">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="disputeDefence"/>
                <xs:sequence>
                    <xs:element minOccurs="0" ref="disputeDefence"/>
                    <xs:element ref="disputeAttachment"/>
                </xs:sequence>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="refund">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
                <xs:element minOccurs="0" ref="branchSpecificExtension"/>
                <xs:element minOccurs="0" ref="reason"/>
                <xs:element minOccurs="0" ref="iban"/>
                <xs:element minOccurs="0" ref="warning"/>
                <xs:element minOccurs="0" ref="orderLines"/>
            </xs:sequence>
            <xs:attribute name="reference"/>
            <xs:attribute name="paymentId"/>
            <xs:attribute name="immediate">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="shopperWebformRefund">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="merchantCode"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="addBackOfficeCode">
        <xs:complexType>
            <xs:attribute name="backOfficeCode" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="authorise">
        <xs:complexType>
            <xs:attribute name="authorisationCode" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="increaseAuthorisation">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="revoke">
        <xs:complexType/>
    </xs:element>
    <xs:element name="extendExpiryDate">
        <xs:complexType/>
    </xs:element>
    <xs:element name="cancelRefund">
        <xs:complexType/>
    </xs:element>
    <xs:element name="cancelRetry">
        <xs:complexType>
            <xs:attribute name="reference"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="voidSale">
        <xs:complexType/>
    </xs:element>
    <xs:element name="approve">
        <xs:complexType/>
    </xs:element>
    <!-- CHILD ELEMENTS OF DEFEND ELEMENT -->
    <!-- ====================================== -->
    <xs:element name="disputeDefence" type="xs:string"/>
    <xs:element name="disputeAttachment">
        <xs:complexType mixed="true">
            <xs:attribute name="fileName" use="required"/>
        </xs:complexType>
    </xs:element>
    <!-- OPTIONAL CHILD ELEMENTS OF REFUND ELEMENT -->
    <!-- ====================================== -->
    <xs:element name="reason" type="xs:string"/>
    <xs:element name="iban" type="xs:string"/>
    <xs:element name="warning" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ORDERINQUIRY ELEMENT -->
    <!-- ====================================== -->
    <xs:element name="cardSearch">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="startDate"/>
            </xs:sequence>
            <xs:attribute name="cardNumber" use="required" type="xs:NMTOKEN"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="dateSearch">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="fromDate"/>
                <xs:element ref="untilDate"/>
            </xs:sequence>
            <xs:attribute name="journalTypeFilter"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="creationDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="fromDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="untilDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="bookingDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="lastDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="startDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="date">
        <xs:complexType>
            <xs:attribute name="dayOfMonth" type="xs:NMTOKEN"/>
            <xs:attribute name="month" use="required" type="xs:NMTOKEN"/>
            <xs:attribute name="year" use="required" type="xs:NMTOKEN"/>
            <xs:attribute name="hour" type="xs:NMTOKEN"/>
            <xs:attribute name="minute" type="xs:NMTOKEN"/>
            <xs:attribute name="second" type="xs:NMTOKEN"/>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF SHOPPER ELEMENT -->
    <!-- ===================================== -->
    <xs:element name="shopperEmailAddress" type="xs:string"/>
    <xs:element name="authenticatedShopperID" type="xs:string"/>
    <xs:element name="shopperDetails">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="firstName"/>
                <xs:element minOccurs="0" ref="lastName"/>
                <xs:element minOccurs="0" ref="telephoneNumber"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="shopperToken">
        <xs:complexType>
            <xs:sequence>
                <xs:choice>
                    <xs:sequence>
                        <xs:element minOccurs="0" ref="maxAmount"/>
                        <xs:element minOccurs="0" ref="subscriptionDetails"/>
                    </xs:sequence>
                    <xs:element minOccurs="0" ref="tokenId"/>
                </xs:choice>
                <xs:element minOccurs="0" ref="label"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="maxAmount">
        <xs:complexType>
            <xs:attribute name="value" use="required" type="xs:NMTOKEN"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="subscriptionDetails">
        <xs:complexType>
            <xs:attribute name="recurrence" default="MONTHLY">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="ANNUAL"/>
                        <xs:enumeration value="MONTHLY"/>
                        <xs:enumeration value="WEEKLY"/>
                        <xs:enumeration value="DAILY"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="label" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ALL ADDRESS ELEMENTS -->
    <!-- ================================= -->
    <!--
        Note: The form of address which contains a street, is deprecated, and should no longer be used.
        The address1 is now optional, as some merchants are supplying an address only for the
        purpose of performing an AVS check, which requires just the postalCode and countryCode.
    -->
    <xs:element name="address">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="firstName"/>
                <xs:element minOccurs="0" ref="lastName"/>
                <xs:choice>
                    <xs:sequence>
                        <xs:element ref="street"/>
                        <xs:element minOccurs="0" ref="houseName"/>
                        <xs:element minOccurs="0" ref="houseNumber"/>
                        <xs:element minOccurs="0" ref="houseNumberExtension"/>
                    </xs:sequence>
                    <xs:sequence>
                        <xs:element minOccurs="0" ref="address1"/>
                        <xs:element minOccurs="0" ref="address2"/>
                        <xs:element minOccurs="0" ref="address3"/>
                    </xs:sequence>
                </xs:choice>
                <xs:element ref="postalCode"/>
                <xs:element minOccurs="0" ref="city"/>
                <xs:element minOccurs="0" ref="state"/>
                <xs:element ref="countryCode"/>
                <xs:element minOccurs="0" ref="telephoneNumber"/>
                <xs:element minOccurs="0" ref="emailAddress"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ADDRESS ELEMENT -->
    <!-- ================================= -->
    <xs:element name="firstName" type="xs:string"/>
    <xs:element name="lastName" type="xs:string"/>
    <xs:element name="street" type="xs:string"/>
    <xs:element name="houseName" type="xs:string"/>
    <xs:element name="houseNumber" type="xs:string"/>
    <xs:element name="houseNumberExtension" type="xs:string"/>
    <xs:element name="address1" type="xs:string"/>
    <xs:element name="address2" type="xs:string"/>
    <xs:element name="address3" type="xs:string"/>
    <xs:element name="postalCode" type="xs:string"/>
    <xs:element name="city" type="xs:string"/>
    <xs:element name="state" type="xs:string"/>
    <xs:element name="countryCode" type="xs:string"/>
    <xs:element name="telephoneNumber" type="xs:string"/>
    <xs:element name="emailAddress" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ALL TICKET-DETAILS ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="ticketDetails">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="bookingSystemTrvl"/>
                <xs:element minOccurs="0" ref="isSeatSale"/>
                <xs:element minOccurs="0" ref="isWithChild"/>
                <xs:element minOccurs="0" ref="purchaseType"/>
                <xs:element minOccurs="0" ref="isPackageTrvl"/>
                <xs:element minOccurs="0" ref="isFlexTicket"/>
                <xs:element minOccurs="0" ref="isInsPurchased"/>
                <xs:element minOccurs="0" ref="secProgTrvl"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL TICKET-DETAILS ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="bookingSystemTrvl" type="xs:string"/>
    <xs:element name="isSeatSale" type="xs:string"/>
    <xs:element name="isWithChild" type="xs:string"/>
    <xs:element name="purchaseType" type="xs:string"/>
    <xs:element name="isPackageTrvl" type="xs:string"/>
    <xs:element name="isFlexTicket" type="xs:string"/>
    <xs:element name="isInsPurchased" type="xs:string"/>
    <xs:element name="secProgTrvl" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ALL HOTEL ELEMENTS -->
    <!-- The hotel data is deprecated for use in BranchSpecificExtension, instead lodging must be used -->
    <!-- ================================= -->
    <xs:element name="hotel">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="checkInDate"/>
                <xs:element ref="checkOutDate"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL CHECKINDATE ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="checkInDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL CHECKOUTDATE ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="checkOutDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL AIRLINE ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="airline">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="airlineName"/>
                <xs:element ref="passenger"/>
                <xs:choice>
                    <xs:element ref="ticket"/>
                    <xs:element ref="nonTicket"/>
                </xs:choice>
                <xs:element ref="agent"/>
                <xs:element minOccurs="0" ref="agentFeeAmounts"/>
            </xs:sequence>
            <xs:attribute name="code" use="required" type="xs:NMTOKEN"/>
            <xs:attribute name="planNumber">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="03"/>
                        <xs:enumeration value="06"/>
                        <xs:enumeration value="09"/>
                        <xs:enumeration value="12"/>
                        <xs:enumeration value="00"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- Code, length 3, consists of the IATA airline code -->
    <!-- CHILD ELEMENTS OF ALL AIRLINENAME ELEMENTS -->
    <!-- ================================= -->
    <!-- Contains the name of an airline as it may appear on a bill -->
    <xs:element name="airlineName" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ALL PASSENGER ELEMENTS -->
    <!-- ================================= -->
    <!-- Contains the name of a passenger -->
    <xs:element name="passenger">
        <xs:complexType mixed="true">
            <xs:attribute name="code" use="required" type="xs:NMTOKEN"/>
            <xs:attribute name="isFreqFlyer">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="0"/>
                        <xs:enumeration value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="passengerSpecificData"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="isFreqFlyer" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ALL TICKET ELEMENTS -->
    <!-- ================================= -->
    <!-- Code consists of the ticket number -->
    <xs:element name="ticket">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="issuer"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="flight"/>
                <xs:element minOccurs="0" ref="ticketAmounts"/>
            </xs:sequence>
            <xs:attribute name="code" use="required" type="xs:NMTOKEN"/>
            <xs:attribute name="restricted" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="0"/>
                        <xs:enumeration value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="passengerCount" type="xs:NMTOKEN"/>
            <xs:attribute name="returnTicket">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="0"/>
                        <xs:enumeration value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL NONTICKET ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="nonTicket">
        <xs:complexType mixed="true">
            <xs:attribute name="subCode" type="xs:NMTOKEN"/>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL AGENT ELEMENTS -->
    <!-- ================================= -->
    <!-- Code, length 8, consists of the IATA travel agency code -->
    <!-- Agent element, contains name of travel agent -->
    <xs:element name="agent">
        <xs:complexType mixed="true">
            <xs:attribute name="code" use="required" type="xs:NMTOKEN"/>
            <xs:attribute name="city"/>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL ISSUER ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="issuer">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="address"/>
                <xs:element minOccurs="0" ref="ticketDetails"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL FLIGHT ELEMENTS -->
    <!-- ================================= -->
    <!-- carrierCode, length 2, consists of the IATA carrier code -->
    <xs:element name="flight">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="departureAirport"/>
                <xs:element ref="arrivalAirport"/>
                <xs:element ref="departureDate"/>
                <xs:element ref="fare"/>
                <xs:element ref="tax"/>
                <xs:element minOccurs="0" ref="reservationItinerary"/>
                <xs:element minOccurs="0" ref="insurance"/>
                <xs:element minOccurs="0" ref="affiliateName"/>
            </xs:sequence>
            <xs:attribute name="carrierCode" use="required" type="xs:NMTOKEN"/>
            <xs:attribute name="stopOverCode">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="0"/>
                        <xs:enumeration value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="flightCode"/>
            <xs:attribute name="isChartered">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="0"/>
                        <xs:enumeration value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="domestic">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="0"/>
                        <xs:enumeration value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL RESERVATIONITINERARY ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="reservationItinerary">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="name"/>
                <xs:element minOccurs="0" ref="departureCity"/>
                <xs:element minOccurs="0" ref="arrivalCity"/>
                <xs:element minOccurs="0" ref="start"/>
                <xs:element minOccurs="0" ref="end"/>
                <xs:element minOccurs="0" ref="deliveryMethod"/>
                <xs:element minOccurs="0" ref="deliveryRecipient"/>
                <xs:element minOccurs="0" ref="addresses"/>
                <xs:element minOccurs="0" ref="parties"/>
                <xs:element minOccurs="0" ref="amount"/>
                <xs:element minOccurs="0" ref="classification"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- information supporting travel legs -->
    <xs:element name="departureCity" type="xs:string"/>
    <xs:element name="arrivalCity" type="xs:string"/>
    <xs:element name="start">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="end">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="deliveryMethod" type="xs:string"/>
    <xs:element name="deliveryRecipient" type="xs:string"/>
    <xs:element name="addresses">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="location"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- structure to allow multiple parties within a reservation, these could be drivers, passengers, hotel guests) -->
    <xs:element name="parties">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="party"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="classification" type="xs:string"/>
    <!-- structure to associate codes with a given address and a type for that address -->
    <xs:element name="location">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="code"/>
                <xs:element minOccurs="0" ref="locationType"/>
                <xs:element minOccurs="0" ref="name"/>
                <xs:element minOccurs="0" ref="address"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="code" type="xs:string"/>
    <!-- can be START, FINISH, HOTEL -->
    <xs:element name="locationType" type="xs:string"/>
    <!-- structure to defining information pertaining to an individual -->
    <xs:element name="party">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="reference"/>
                <xs:element minOccurs="0" ref="firstName"/>
                <xs:element minOccurs="0" ref="lastName"/>
                <xs:element minOccurs="0" ref="salutation"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- can be mr | mrs | ms | (blank value) -->
    <xs:element name="salutation" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ALL INSURANCE ELEMENTS -->
    <!-- ================================= -->
    <!-- Structure to transfer insurance information -->
    <xs:element name="insurance">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="insuranceCompany"/>
                <xs:element minOccurs="0" ref="insuranceType"/>
                <xs:element minOccurs="0" ref="amount"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="insuranceCompany" type="xs:string"/>
    <!-- can be cancellation | travel | cancellation_travel | bankruptcy -->
    <xs:element name="insuranceType" type="xs:string"/>
    <xs:element name="affiliateName" type="xs:string"/>
    <xs:element name="affiliateAddress">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="city"/>
                <xs:element ref="state"/>
                <xs:element ref="countryCode"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="reservationCount" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ALL DEPARTUREAIRPORT ELEMENTS -->
    <!-- ================================= -->
    <!-- Consists of the 3 chars IATA airport code -->
    <xs:element name="departureAirport" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ALL ARRIVALAIRPORT ELEMENTS -->
    <!-- ================================= -->
    <!-- Consists of the 3 chars IATA airport code -->
    <xs:element name="arrivalAirport" type="xs:string"/>
    <!-- CHILD ELEMENTS OF ALL DEPARTUREDATE ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="departureDate">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="date"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL FARE ELEMENTS -->
    <!-- ================================= -->
    <!--
        class consists of the single character IATA
        travel class code
    -->
    <xs:element name="fare">
        <xs:complexType>
            <xs:attribute name="class" use="required" type="xs:NMTOKEN"/>
            <xs:attribute name="basis" type="xs:NMTOKEN"/>
        </xs:complexType>
    </xs:element>
    <!-- CHILD ELEMENTS OF ALL FARE ELEMENTS -->
    <!-- ================================= -->
    <xs:element name="tax">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="amount"/>
